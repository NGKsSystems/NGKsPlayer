{
  "name": "ngksplayer",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "appId": "ngks.player",
  "scripts": {
    "vite": "vite",
    "electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 NODE_ENV=development ./node_modules/.bin/electron .",
    "dev": "concurrently -k \"pnpm run vite\" \"wait-on http://localhost:5173 && pnpm run electron\"",
    "dev:quiet": "cross-env LOG_LEVEL=WARN concurrently -k \"pnpm run vite\" \"wait-on http://localhost:5173 && pnpm run electron\"",
    "dev:silent": "cross-env LOG_LEVEL=ERROR concurrently -k \"pnpm run vite\" \"wait-on http://localhost:5173 && pnpm run electron\"",
    "dev:verbose": "cross-env LOG_LEVEL=VERBOSE concurrently -k \"pnpm run vite\" \"wait-on http://localhost:5173 && pnpm run electron\"",
    "dev:clean": "pnpm run cleanup && pnpm run dev",
    "cleanup": "taskkill /f /im electron.exe 2>nul || echo No Electron processes to kill && taskkill /f /im node.exe 2>nul || echo No Node processes to kill",
    "kill-processes": "taskkill /f /im electron.exe && taskkill /f /im node.exe",
    "build": "vite build && electron-builder",
    "rebuild": "pnpm exec electron-rebuild -f -m node_modules/better-sqlite3 -m node_modules/sqlite3",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" pnpm exec jest",
    "test:watch": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" pnpm exec jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" pnpm exec jest --coverage",
    "test:audio": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" pnpm exec jest src/tests/audio/",
    "test:components": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" pnpm exec jest src/tests/components/",
    "test:integration": "pnpm exec jest --testPathPattern=integration",
    "test-autotagger": "node src/analysis/quickTest.js",
    "test-autotagger-full": "node src/analysis/AutoTaggerTester.js",
    "test:robot": "node run-testing-robot.js",
    "test:robot:audio": "node run-testing-robot.js -- --audio",
    "test:robot:ui": "node run-testing-robot.js -- --ui",
    "test:robot:quick": "node run-testing-robot.js -- --quick",
    "test:robot:verbose": "node run-testing-robot.js -- --verbose",
    "calibrate": "node calibrate-analyzer.js",
    "calibrate:verbose": "node calibrate-analyzer.js --verbose"
  },
  "dependencies": {
    "@domchristie/needles": "^0.0.2-1",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "better-sqlite3": "^9.6.0",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.3.0",
    "electron-store": "^8.1.0",
    "essentia.js": "^0.1.3",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "lucide-react": "^0.546.0",
    "meyda": "^5.6.3",
    "music-metadata": "^10.9.1",
    "node-id3": "^0.2.6",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "speaker": "^0.5.5",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.5.2",
    "tone": "^15.1.22",
    "web-audio-api": "^0.2.2",
    "web-audio-beat-detector": "^8.2.30",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^30.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.4.6",
    "electron-builder": "^24.6.4",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.9",
    "vite": "^7.1.5",
    "wait-on": "^7.2.0"
  }
}
